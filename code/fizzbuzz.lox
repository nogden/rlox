fun modulo(x, y) {
    return x - (y * (x / y))
}

fun fizzbuzz(count) {
    for (i = 0; i < count; ++i) {
        if (modulo(i, 15) == 0) {
            print "FizzBuzz";
        } else if (modulo(i, 5)) {
            print "Fizz";
        } else if (modulo(i, 3)) {
            print "Buzz";
        }
    }
}

fizzbuzz(100);
